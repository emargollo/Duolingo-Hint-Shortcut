{
  "manifest_version": 2,
  "name": "Duolingo Hint Shortcut",
  "version": "0.0.1",
  "description": "Adds a keyboard shortcut to display hints for the words.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.duolingo.com/skill/*"],
      "js": ["shortcuts.js"]
    }
  ]
}